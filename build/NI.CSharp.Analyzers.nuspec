<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>NI.CSharp.Analyzers</id>
    <!--TODO: author and owner may have to change if we go to nuget.org -->
    <authors>NI</authors>
    <owners>NI</owners>
    <copyright>Copyright 2020 National Instruments Corporation</copyright>
    <description>NI's code analyzers and rulesets for C# projects.</description>
    <summary>NI's code analyzers and rulesets for C# projects.</summary>
    <icon>images\icon.png</icon>
    <repository type="git" url="https://github.com/ni/csharp" />
    <projectUrl>https://github.com/ni/csharp</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="file">LICENSE.txt</license>
    <dependencies>
      <group targetFramework="netstandard1.3">
        <dependency id="Microsoft.CodeAnalysis.FxCopAnalyzers" version="2.6.3" />
        <dependency id="Microsoft.CodeQuality.Analyzers" version="2.6.3" />
        <dependency id="Microsoft.NetCore.Analyzers" version="2.6.3" />
        <dependency id="Microsoft.NetFramework.Analyzers" version="2.6.3" />
        <dependency id="Microsoft.VisualStudio.Threading.Analyzers" version="16.3.52" />
        <dependency id="Roslynator.Analyzers" version="2.1.0" />
        <dependency id="StyleCop.Analyzers" version="1.1.118" />
        <dependency id="Text.Analyzers" version="2.6.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\.binaries\Release\NationalInstruments.Analyzers\**\*.dll" target="lib" />
    <file src="..\configuration\NI.CSharp.Analyzers.targets" target="build" />
    <file src="..\rulesets\*.ruleset" target="content" />
    <file src="..\configuration\NI1704_AdditionalSpellingDictionary.dic" target="content" />
    <file src="..\build\PathPropertyGeneration.txt" target="tools" />
    <file src="..\docs\icon.png" target="images" />
    <file src="..\LICENSE.txt" target="" />
    <file src="..\README.md" target="" />
  </files>
</package>
